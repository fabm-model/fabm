!-------------------------------------------------------------------------------
! fabm_nml
!-------------------------------------------------------------------------------
! models             [string array]
!                     a comma-separated list of one or model model names
!-------------------------------------------------------------------------------

&fabm_nml
   models = 'examples_npzd_nut','examples_npzd_det','examples_npzd_phy','examples_npzd_zoo'
/

!-------------------------------------------------------------------------------
! examples_npzd_nut
!-------------------------------------------------------------------------------
&examples_npzd_nut
   n_initial = 4.5,
/

!-------------------------------------------------------------------------------
! examples_npzd_det
!-------------------------------------------------------------------------------
&examples_npzd_det
   d_initial = 4.5,
   w_d = -5.0,
   rdn = 0.003,
   kc  = 0.03,
   mineralisation_target_variable='examples_npzd_nut_nut'
/

!-------------------------------------------------------------------------------
! examples_npzd_phy
!-------------------------------------------------------------------------------
&examples_npzd_phy
   p_initial = 1e-15,
   p0 = 0.0225,
   w_p = -1.0,
   kc  = 0.03,
   I_min = 25.0,
   rmax = 1.0,
   alpha = 1.35,
   rpn = 0.01,
   rpdu = 0.02,
   rpdl = 0.1,
   excretion_target_variable='examples_npzd_nut_nut',
   mortality_target_variable='examples_npzd_det_det',
   uptake_target_variable='examples_npzd_nut_nut'
/

!-------------------------------------------------------------------------------
! examples_npzd_zoo
!-------------------------------------------------------------------------------
&examples_npzd_zoo
   z_initial = 1e-15,
   z0 = 0.0225,
   gmax = 0.2,
   Iv = 1.1,
   rzn = 0.01,
   rzd = 0.02,
   excretion_target_variable='examples_npzd_nut_nut',
   mortality_target_variable='examples_npzd_det_det',
   grazing_target_variable='examples_npzd_phy_phy'   
/
